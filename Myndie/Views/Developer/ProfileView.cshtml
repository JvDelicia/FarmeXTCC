
@{
    Layout = "~/Views/Shared/_DeveloperLayout.cshtml";
}


    
    <link href="~/assets/css/cavalo.css" rel="stylesheet" />

    <link href="~/Content/style.css" rel="stylesheet" />


    <div id="carrousel">
        <div class="container">
            <div class="title col-md-12 text-center"> Your last uploads</div>
            <div class="col-md-12">
                <div class="owl-carousel owl-theme">
                    @foreach (var app in ViewBag.DevGames)
                    {
                        <a href="~/Application/EditGame/@app.Id">
                            <img src="@app.ImageUrl" alt="@App.Name">
                        </a>
                    }
                </div>
            </div>
            <div class="text-center">
                <div class="status">Click To edit</div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/cavalo1.js"></script>
    <script src="~/Scripts/cavalo2.js"></script>

    <script src="~/Scripts/index.js"></script>






