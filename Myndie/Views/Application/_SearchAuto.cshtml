<form action="~/Application/Search" autocomplete="on" id="FormSearch">
    <div class="myndie-input" id="searchIndex">
        <input type="text" id="search" name="search" placeholder="Search">
    </div>
</form>


<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<!-- Bootstrap TypeAhead-->
<script src="../assets/myndie/js/bootstrap3-typeahead.min.js"></script>
<!-- TypeAhead init-->
<script src="../assets/myndie/js/forms-autocomplete.js"></script>
<script src="~/Scripts/typeahead.jquery.min.js"></script>
<script src="~/Scripts/typeahead.mvc.model.js"></script>
<script src="~/Scripts/bloodhound.min.js"></script>
<script src="~/Scripts/typeahead.bundle.js"></script>
<script>
    var values = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.AppName));
   

    $(function () {
        var $search = $('#search').hide(),
            values = [@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.AppName))];

        var apps = new Bloodhound({
            datumTokenizer: Bloodhound.tokenizers.obj.whitespace('Name'),
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            local: values
            //,prefetch: '/path/to/prefetch'
            //,remote: {/* You can use this for ajax call*/ }
        });

        apps.initialize();

        $('#search').typeahead({ highlight: true, minLength: 2 }, {
            name: 'values', displayKey: 'Name', source: values.ttAdapter()
        })
            .on("typeahead:selected", function (obj, app) {
                $search.html("Selected Company: " + JSON.stringify(app)).show();
            });

    });
    console.log($search);
</script>
